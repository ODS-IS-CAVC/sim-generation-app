{
  "name": "dll-catalog-frontend",
  "version": "0.0.0",
  "scripts": {
    "loc": "vite --port 5050 --mode loc",
    "dev": "vite --port 5050 --mode dev",
    "stg": "vite --port 5050 --mode stg",
    "mock": "vite --port 5050 --mode mock",
    "preview": "vite preview --port 5050",
    "typecheck": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "audit": "npm audit --audit-level=high > ./reports/audit.txt",
    "lint": "npm run eslint && npm run stylelint",
    "eslint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --ignore-path .gitignore -f node_modules/eslint-detailed-reporter/lib/detailed.js -o ./reports/eslint.html",
    "stylelint": "stylelint **/*.{vue,css,scss} > ./reports/stylelint.txt",
    "build": "vue-tsc --noEmit && vite build",
    "build:loc": "vue-tsc --noEmit && vite build --mode loc",
    "build:dev": "vue-tsc --noEmit && vite build --mode dev",
    "build:stg": "vue-tsc --noEmit && vite build --mode stg",
    "build:prd": "vue-tsc --noEmit && vite build --mode prd",
    "generate-client": "openapi-generator-cli --openapitools ./config/openapitools.json   generate -g typescript-axios -i ./api-specification.json -o ./src/generated/api-client"
  },
  "dependencies": {
    "@mdi/font": "^7.2.96",
    "@vee-validate/yup": "^4.11.1",
    "@vuepic/vue-datepicker": "^8.8.1",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^2.0.0",
    "pinia": "^2.1.6",
    "pinia-plugin-persistedstate": "^3.2.0",
    "vee-validate": "^4.11.1",
    "video.js": "^8.19.1",
    "vue": "^3.3.4",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.2.4",
    "vuetify": "^3.6.7",
    "vuex": "^4.1.0",
    "yup": "^1.2.0",
    "yup-locale-ja": "^1.0.0"
  },
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.13.4",
    "@pinia/testing": "^0.1.3",
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node18": "^18.2.0",
    "@types/jsdom": "^21.1.1",
    "@types/node": "^20.5.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitejs/plugin-vue-jsx": "^3.0.1",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "@vue/tsconfig": "^0.4.0",
    "autoprefixer": "^10.4.15",
    "eslint": "^8.47.0",
    "eslint-detailed-reporter": "^0.9.0",
    "eslint-plugin-vue": "^9.17.0",
    "flush-promises": "^1.0.2",
    "jsdom": "^22.1.0",
    "loglevel": "^1.8.1",
    "mochawesome": "^7.1.3",
    "postcss": "^8.4.27",
    "postcss-html": "^1.5.0",
    "postcss-nesting": "^12.0.1",
    "postcss-scss": "^4.0.6",
    "prettier": "^3.0.1",
    "resize-observer-polyfill": "^1.5.1",
    "sass": "^1.65.1",
    "start-server-and-test": "^2.0.0",
    "stylelint": "^15.10.2",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "stylelint-prettier": "^4.0.2",
    "stylelint-scss": "^5.1.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.9",
    "vite-plugin-compression": "^0.5.1",
    "vite-plugin-vuetify": "^1.0.2",
    "vue-tsc": "^2.1.6",
    "wait-for-expect": "^3.0.2"
  }
}
